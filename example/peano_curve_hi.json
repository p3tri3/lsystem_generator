{
  "name": "Peano Curve (high iteration)",
  "axiom": "X",
  "iterations": 5,
  "rules": {
    "X": "XFYFX+F+YFXFY-F-XFYFX",
    "Y": "YFXFY-F-XFYFX+F+YFXFY"
  },
  "turtle": {
    "angle": 90,
    "step": 1.0,
    "start": {
      "x": 0,
      "y": 0,
      "heading": 0
    },
    "commands": {
      "F": {
        "type": "forward",
        "draw": true
      },
      "X": {
        "type": "noop"
      },
      "Y": {
        "type": "noop"
      },
      "+": {
        "type": "turn",
        "direction": 1,
        "angle": 1
      },
      "-": {
        "type": "turn",
        "direction": -1,
        "angle": 1
      }
    }
  },
  "svg": {
    "margin": 0.03,
    "precision": 6,
    "flip_y": true,
    "style": {
      "stroke": "#000",
      "stroke_width": 0.15,
      "fill": "none",
      "stroke_linecap": "round",
      "stroke_linejoin": "round"
    }
  }
}
