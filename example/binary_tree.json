{
  "name": "Binary Tree",
  "axiom": "F",
  "iterations": 6,
  "rules": {
    "F": "F[+F]F[-F]F"
  },
  "turtle": {
    "angle": 20,
    "step": 1.0,
    "start": {
      "x": 0,
      "y": 0,
      "heading": 90
    },
    "commands": {
      "F": {
        "type": "forward",
        "draw": true
      },
      "+": {
        "type": "turn",
        "direction": 1,
        "angle": 1
      },
      "-": {
        "type": "turn",
        "direction": -1,
        "angle": 1
      },
      "[": {
        "type": "push"
      },
      "]": {
        "type": "pop"
      }
    }
  },
  "svg": {
    "margin": 0.02,
    "precision": 6,
    "flip_y": true,
    "style": {
      "stroke": "#000",
      "stroke_width": 0.2,
      "fill": "none",
      "stroke_linecap": "round",
      "stroke_linejoin": "round"
    }
  }
}
