{
  "name": "Moore Curve (high iteration, closed)",
  "axiom": "LFL+F+LFL",
  "iterations": 6,
  "rules": {
    "L": "-RF+LFL+FR-",
    "R": "+LF-RFR-FL+"
  },
  "turtle": {
    "angle": 90,
    "step": 1.0,
    "start": {
      "x": 0,
      "y": 0,
      "heading": 0
    },
    "commands": {
      "F": {
        "type": "forward",
        "draw": true
      },
      "L": {
        "type": "noop"
      },
      "R": {
        "type": "noop"
      },
      "+": {
        "type": "turn",
        "direction": 1,
        "angle": 1
      },
      "-": {
        "type": "turn",
        "direction": -1,
        "angle": 1
      }
    }
  },
  "svg": {
    "margin": 0.03,
    "precision": 6,
    "flip_y": true,
    "style": {
      "stroke": "#000",
      "stroke_width": 0.15,
      "fill": "none",
      "stroke_linecap": "round",
      "stroke_linejoin": "round"
    }
  }
}
